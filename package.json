{
  "name": "dataopener",
  "scripts": {
    "develop-server": "yarn build-web; NODE_ENV='development' nodemon ./server/app.js --exec babel-node --watch server",
    "develop-web": "NODE_ENV='development' NODE_WATCH_WEB=1 nodemon ./server/app.js --exec babel-node --watch web",
    "build": "yarn build-server; yarn build-web",
    "build-server": "babel server -d dist --ignore '/__tests__/,/__mocks__/';",
    "build-web": "next build ./web",
    "test": "jest ./web ./server --coverage --detectOpenHandles",
    "test-eslint": "eslint ./",
    "test-server": "jest ./server --coverage --detectOpenHandles",
    "test-web": "jest ./web --coverage --detectOpenHandles",
    "start": "node ./dist/app.js"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime-corejs2": "^7.9.6",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "base-x": "^3.0.7",
    "buffer": "^5.4.3",
    "copy-to-clipboard": "^3.2.0",
    "cors": "^2.8.5",
    "cz-conventional-changelog": "^3.0.2",
    "d3": "^5.15.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "express": "^4.17.3",
    "formik": "^2.0.8",
    "fuse.js": "^3.4.6",
    "global": "^4.4.0",
    "googleapis": "^47.0.0",
    "graphql": "^14.5.8",
    "jest": "^25.1.0",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "luxon": "^1.22.0",
    "markdown-it": "^10.0.0",
    "next": "^9.3.2",
    "nextjs-apollo": "^1.7.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "recharts": "^2.0.0-beta.1",
    "styled-components": "^4.4.0",
    "subscriptions-transport-ws": "^0.9.16",
    "yup": "^0.28.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
